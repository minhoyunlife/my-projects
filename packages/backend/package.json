{
  "name": "@my-projects/backend",
  "version": "1.0.0",
  "description": "Backend for MY Projects",
  "author": "Minho Yun",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"./**/*.ts\" --fix",
    "test:no-deps": "vitest run -c \"test/vitest.config.no-deps.ts\"",
    "test:deps": "vitest run -c \"test/vitest.config.deps.ts\"",
    "test:watch:no-deps": "vitest watch -c \"test/vitest.config.no-deps.ts\"",
    "test:watch:deps": "vitest watch -c \"test/vitest.config.deps.ts\"",
    "test:all": "npm run test:no-deps && npm run test:deps"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.688.0",
    "@aws-sdk/s3-request-presigner": "^3.688.0",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.4",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "multer": "1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "pg": "^8.13.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.35",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.3",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.2"
  }
}
